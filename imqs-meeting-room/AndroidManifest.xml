<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.za.imqs.meetingroom"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditMeeting"
            android:label="@string/title_activity_edit_meeting"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="co.za.imqs.meetingroom.MainActivity" />
        </activity>
        <activity
            android:name=".EditRoomActivity"
            android:label="@string/title_activity_edit_room"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="co.za.imqs.meetingroom.MainActivity" />
        </activity>
        <activity
            android:name=".EditMeetingActivity"
            android:label="@string/title_activity_edit_meeting"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="co.za.imqs.meetingroom.MainActivity" />
        </activity>
    </application>

</manifest>
